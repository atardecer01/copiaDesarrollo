{
    "plugins": ["complexity"],
    "parser": "babel-eslint",
    "extends": "next",
    "rules": {
      "complexity": ["warn", 5],
      "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off"
    },
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    }
  }